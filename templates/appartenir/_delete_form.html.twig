<form method="post" action="{{ path('appartenir_delete', {'id': appartenir.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ appartenir.id) }}">
    <button class="btn">Delete</button>
</form>
